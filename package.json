{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "preview": "vite preview",
        "build-tokens": "npx style-dictionary build --config style-dictionary.config.cjs",
        "tokens": "npm run build-tokens && npm run dev",
        "dev-tokens": "npm run build-tokens && npm run dev",
        "watch-tokens": "npx style-dictionary build --config style-dictionary.config.cjs --watch",
        "clean-tokens": "rm -rf build/ && npm run build-tokens"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "autoprefixer": "^10.4.7",
        "laravel-vite-plugin": "^0.8.0",
        "postcss": "^8.4.14",
        "style-dictionary": "^3.9.2",
        "tailwindcss": "^3.1.0",
        "vite": "^4.0.0"
    },
    "dependencies": {
        "axios": "^1.10.0",
        "vue": "^3.2.31"
    }
}
